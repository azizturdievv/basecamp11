
 <%= render "devise/shared/error_messages", resource: resource %>


<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
       <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
 
				<form class="login100-form validate-form">
        
					<span class="login100-form-title p-b-26">
					   <h2>Sign up</h2>
					</span>
					<span class="login100-form-title p-b-48">
						<i class="zmdi zmdi-font"></i>
					</span>

					<div class="wrap-input100 validate-input" data-validate = "Valid email is: a@b.c">

          
						<%= f.email_field :email, autofocus: true, autocomplete: "email",placeholder: "E-mail", required: true  %>
						
					</div>


					<div class="wrap-input100 validate-input" data-validate="Enter password">
						<span class="btn-show-pass">
							<i class="zmdi zmdi-eye"></i>
						</span>
                 <%= f.label :password %>
    <% if @minimum_password_length %>
    <em class="none">(<%= @minimum_password_length %> characters minimum)</em>
    <% end %>
    <%= f.password_field :password, autocomplete: "new-password",placeholder: "Password", required: true  %>
		
     
						
					</div>


<div class="wrap-input100 validate-input" data-validate="Enter password">
						<span class="btn-show-pass">
							<i class="zmdi zmdi-eye"></i>
						</span>
                 
   
    
		<%= f.password_field :password_confirmation, autocomplete: "new-password",placeholder: "Password",required: true  %>
     
						
					</div>
<div class="container-login100-form-btn">
						<div class="wrap-login100-form-btn">
							<div class="login100-form-bgbtn"></div>
							<button class="login100-form-btn">
               <%= f.submit "Sign up",class: "register1" %>
							
							</button>
						</div>
					</div>

					<div class="text-center p-t-115">
						<span class="txt1">
							Do yo have an account?
						</span>

						<a class="txt2" href="#">
							<%= render "devise/shared/links" %>
						</a>
					</div>
        
				</form>
        	  <% end %>
			</div>
		</div>
	</div>
 

<style>

</style>

